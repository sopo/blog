{
    "builds": [
      {
        "src": "server.cjs",
        "use": "@vercel/node",
        "config": {
         
        }
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "buildCommand": "npm run build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.cjs"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  